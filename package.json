{
  "name": "games-uploader",
  "version": "1.0.0",
  "description": "Uploads clips of games from Medal.tv or other screen recorders!",
  "homepage": "https://github.com/viradex/games-uploader#readme",
  "bugs": {
    "url": "https://github.com/viradex/games-uploader/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viradex/games-uploader.git"
  },
  "license": "MIT",
  "author": "Arnav Thorat",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.arnav.gamesuploader",
    "productName": "Games Uploader",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    }
  },
  "dependencies": {
    "ffmpeg-static": "^5.2.0",
    "googleapis": "^164.1.0"
  },
  "devDependencies": {
    "electron-builder": "^26.0.12"
  }
}
